  <div class="container">
    <div class="app-name">
      <h1>CONTACT APP</h1>
      <i class="fa fa-info-circle" aria-hidden="true"></i>
    </div>
    <main class="navbar">
      <div class="All-contacts">All Contacts</div>
      <div class="row">
        <a href="/add-contact">
          <div class="fa-fa">
            <i class="fa fa-plus-circle" aria-hidden="true"></i>
          </div>
          <div class="add-new">Add New</div>
      </div>
      </a>
    </main>
