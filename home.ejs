  <div class="table-container1">
    <table>
      <thead>
        <tr>
          <th>Sl No.</th>
          <th>First Name</th>
          <th>Last Name</th>
          <th>Email</th>
          <th>Phone</th>
          <th>Actions</th>
        </tr>
      </thead>
      <div class="t-body">
        <tbody>
          <% var slno =1%>
          <% contacts.forEach(contact=> { %>
            <tr>
              <td><%=slno++%>
              </td>
              <td><%=contact.first_Name %></td>
              <td><%=contact.last_Name %></td>
              <td><%=contact.email %></td>
              <td><%=contact.phone %></td>
              <td>
                <div class="fa-fa1">
                  <a href="/show-contact/<%=contact._id %>" onclick="confirm(`Are you sure!You want show `)" title="Show"><i
                      class="fa fa-eye"></i></a>

                  <a href="/update-contact/<%=contact._id %>" onclick="confirm(`Are you sure! You want update`)" title="update"><i
                      class="fa fa-edit"></i></a>

                  <a href="/delete-contact/<%=contact._id %>" title="Delete" onclick="confirm('Are you sure?!You want delete')"><i
                      class="fa fa-times"></i></a>
                </div>
              </td>
            </tr>
         <% })%>

